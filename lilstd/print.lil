fn printf (ptr(i8)...)=>i32 extern;
fn puts (ptr(i8))=>i32 extern;
#needs "string.lil";

#export fn print(var.bool|i64|f64|ptr(i8)|@string value) {
	if value => bool {
		if value {
			puts `true`;
		} else {
			puts `false`;
		}
	}
	if value => i64 {
		printf(`%li\n`, value);
	}
	if value => f64 {
		printf(`%lf\n`, value);
	}
	if value => ptr(i8) {
		puts value;
	}
	if value => @string {
		puts value.cstr();
	}
};
